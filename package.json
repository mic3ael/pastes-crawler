{
  "name": "crawlers",
  "version": "1.0.0",
  "description": "Example of creating a function that runs as a cron job using the serverless `schedule` event",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "serverless webpack --stage local",
    "local": "./node_modules/serverless/bin/serverless.js offline start --stage local",
    "deploy": "serverless deploy --stage local",
    "watch": "nodemon -w src -e '.*' -x 'npm run build'",
    "start": "npm run deploy && npm run watch"
  },
  "dependencies": {
    "aws-sdk": "~2.1244.0",
    "axios": "~1.1.3",
    "cheerio": "~1.0.0-rc.12",
    "dynamodb-admin": "~4.5.0",
    "moment": "~2.29.4",
    "node-fetch": "~2.6.7",
    "pino": "~8.7.0",
    "user-agents": "~1.0.1187",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "~2.0.20",
    "serverless": "~3.23.0",
    "serverless-localstack": "~1.0.1",
    "serverless-webpack": "~5.10.0",
    "webpack": "~5.74.0"
  },
  "engines": {
    "node": "~16.16.0"
  }
}
